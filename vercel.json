{
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "vite",
  "outputDirectory": "dist",
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/github",
      "destination": "https://github.com/ratul1918",
      "permanent": false
    },
    {
      "source": "/linkedin",
      "destination": "https://www.linkedin.com/in/rafiur-rahman-9b3723336",
      "permanent": false
    }
  ]
}
